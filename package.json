{
  "name": "nuxt-feathers-vuex",
  "version": "0.0.7",
  "description": "Feathers-Vuex module for Nuxt",
  "license": "MIT",
  "contributors": [
    {
      "name": "Gusto JS <gustojs@protonmail.com>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/gustojs/nuxt-feathers-vuex",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "standard-version": {
    "scripts": {
      "prebump": "rm -f package-lock.json"
    }
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.3.0",
    "@feathersjs/feathers": "^4.3.0",
    "@feathersjs/socketio-client": "^1.1.0",
    "cookie-storage": "^3.1.0",
    "feathers-vuex": "^2.0.0-pre.75",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "nuxt": "latest",
    "standard-version": "latest"
  }
}
